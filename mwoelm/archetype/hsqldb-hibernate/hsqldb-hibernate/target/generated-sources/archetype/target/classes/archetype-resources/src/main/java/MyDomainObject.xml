#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="${package}">

    <class name="MyDomainObject" table="TextObject">
        <id name="id" column="Object_ID">
            <generator class="increment"/>
        </id>
        <property name="text" type="string" column="Text"/>
    </class>

</hibernate-mapping>